{
  "name": "everywhere-cli",
  "version": "0.0.1",
  "keywords": [
    "cli",
    "ec",
    "everywhere-cli",
    "everywhere-computer",
    "fission"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/evyerwhere-computer/evyerwhere-cli",
    "directory": "./"
  },
  "author": "Fission Team <support@fission.codes>",
  "license": "MIT",
  "bin": {
    "everywhere-cli": "./dist/index.js"
  },
  "files": [
    "bin",
    "dist"
  ],
  "bugs": {
    "url": "https://github.com/evyerwhere-computer/everywhere-cli/issues"
  },
  "type": "module",
  "scripts": {
    "start": "node ./bin/run.js",
    "dev": "ncc build ./index.ts -w -o dist/",
    "prerelease": "rimraf ./dist/",
    "release": "ncc build ./index.ts -o ./dist/ --minify --no-cache --no-source-map-register",
    "homestar": "homestar start -c ./config/settings.toml"
  },
  "devDependencies": {
    "@vercel/ncc": "^0.38.1",
    "async-retry": "^1.3.3",
    "chalk": "^5.3.0",
    "commander": "^11.1.0",
    "cross-spawn": "^7.0.3",
    "got": "^14.1.0",
    "prettier": "^3.2.4",
    "prompts": "^2.4.2",
    "rimraf": "^5.0.5",
    "tar": "^6.2.0",
    "typescript": "^5.3.3",
    "update-check": "^1.5.4",
    "validate-npm-package-name": "^5.0.0"
  },
  "engines": {
    "node": ">=12.22.0"
  },
  "dependencies": {
    "@helia/unixfs": "^3.0.0",
    "blockstore-fs": "^1.1.9",
    "execa": "5.1.1",
    "express": "^4.18.2",
    "fastest-levenshtein": "^1.0.16",
    "helia": "^4.0.1",
    "homestar-runtime": "^0.0.8",
    "inquirer": "^9.2.13",
    "is-wsl": "^3.1.0",
    "os": "^0.1.2",
    "update-notifier": "^7.0.0"
  }
}
